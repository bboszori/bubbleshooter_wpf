<Window x:Class="Oenik_prog3_2017osz_iapw0k.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oenik_prog3_2017osz_iapw0k"
        mc:Ignorable="d"
        Title="Bubble Shooter" Height="600" Width="315" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow" ResizeMode="CanMinimize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="400"/>
            <RowDefinition Height="100"/>
        </Grid.RowDefinitions>

        <local:GameFrameworkElement Grid.Row="1" KeyDown="GameFrameworkElement_KeyDown" />
        <Border Grid.Row="0" Background="DarkSlateGray"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="96" Margin="10,0,0,0" TextWrapping="Wrap" Text="Bubble Shooter" VerticalAlignment="Top" Width="289" Foreground="White" Background="DarkSlateGray" BorderThickness="0" HorizontalContentAlignment="Center" FontSize="38" FontWeight="ExtraBold"/>

    </Grid>
</Window>
